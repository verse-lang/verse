<modifier> := \*|\[($[0-9]+)\]
<typeName> := [A-Z][a-zA-Z0-9]*
<type> := <typeName>(\<<type>+\>)?<modifier>*
<identifier> := [a-zA-Z][a-zA-Z0-9]*

<program> := <line>*
<line> := <statement><eol>
<eol> := <<break>>|$$$
<statement> := <varDec>|<structDec>|<classDec>|<method>|<if>|<while>|<for>|<expression>
<varDec> := ($#?)<type> [a-z][a-zA-Z0-9]*
//<typedVarDec> := <typedVarDecInit> | <typedVarDecNoInit>
<if> := <<=if>> <expression>
