class Vec2<T> T x y|
	Vec2<T> ops ~~ + - / * & | ^ Vec2<T> b
		return new Vec2<T>(x ~~ b.x, y ~~ b.y)
	Vec2<T> ops ~~ + - / * & | ^ Num b
		return new Vec2<T>(x ~~ b, y ~~ b)
	ops ~~ += -= /= *= &= |= ^= Vec2<T> b
		x ~~ b.x
		y ~~ b.y
	ops ~~ += -= /= *= &= |= ^= Num b
		x ~~ b
		y ~~ b
	op ~
		x = ~x
		y = ~y
